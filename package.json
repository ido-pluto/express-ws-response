{
  "name": "express-ws-response",
  "version": "1.0.0",
  "description": "Avoid HTTP Timeouts with WebSocket response, for Express.js",
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "node": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "browser": {
        "import": "./dist/browser.js",
        "types": "./dist/browser.d.ts",
        "default": "./dist/browser.js"
      }
    },
    "./node": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./browser": {
      "import": "./dist/browser.js",
      "types": "./dist/browser.d.ts",
      "default": "./dist/browser.js"
    },
    "./dist/index.js": "./dist/index.js",
    "./dist/browser.js": "./dist/browser.js"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "rm -r dist; tsc",
    "prepack": "npm run build",
    "release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ido-pluto/express-ws-response.git"
  },
  "keywords": [
    "express",
    "websocket",
    "response",
    "ngnix",
    "proxy",
    "timeout",
    "http",
    "express.js",
    "middleware",
    "bson",
    "streaming",
    "cross-origin",
    "CORS",
    "real-time",
    "data-encoding"
  ],
  "author": "Ido S.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ido-pluto/express-ws-response/issues"
  },
  "homepage": "https://github.com/ido-pluto/express-ws-response#readme",
  "funding": "https://github.com/sponsors/ido-pluto",
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/mime-types": "^2.1.4",
    "@types/once": "^1.4.5",
    "@types/ws": "^8.5.14",
    "express": "^4.21.2",
    "semantic-release": "^24.0.0",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "bson": "^6.10.3",
    "mime-types": "^2.1.35",
    "node-mocks-http": "^1.16.2",
    "once": "^1.4.0",
    "ws": "^8.18.1",
    "zod": "^3.24.2",
    "zod-error": "^1.5.0"
  }
}
